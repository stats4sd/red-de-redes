{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "backpack/crud": "4.1.*",
        "backpack/settings": "^3.0.0",
        "barryvdh/laravel-elfinder": "^0.4.3",
        "calebporzio/parental": "0.9",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "khill/lavacharts": "3.1.*",
        "laravel/framework": "6.0",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^6.0",
        "maatwebsite/excel": "^3.1",
        "mopo922/laravel-treats": "^1.0",
        "prologue/alerts": "^0.4.3",
        "pusher/pusher-php-server": "^4.0",
        "spatie/laravel-translatable": "^4.0",
        "symfony/process": "4.3.*",
        "symfony/psr-http-message-bridge": "*",
        "tightenco/parental": "0.8",
        "twbs/bootstrap": "4.2.1",
        "usmanhalalit/laracsv": "1.*@dev",
        "yajra/laravel-datatables-oracle": "^9.0"
    },
    "require-dev": {
        "backpack/generators": "^3.0",
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "1.4.*",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "^1.0",
        "laravel/ui": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "webfactor/laravel-generators": "^3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
